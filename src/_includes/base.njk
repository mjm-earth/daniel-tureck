<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>{{ pageTitle or site.title }}</title>
  <meta name="description" content="{{ pageDescription or site.description }}">
  <meta name="author" content="{{ bio.name }}">
  <meta name="theme-color" content="#F5F0E8">
  <meta name="robots" content="index, follow">

  <!-- Open Graph -->
  <meta property="og:title" content="{{ pageTitle or site.title }}">
  <meta property="og:description" content="{{ pageDescription or site.description }}">
  <meta property="og:image" content="{{ site.url }}{{ bio.photo | url }}">
  <meta property="og:url" content="{{ site.url }}{{ page.url }}">
  <meta property="og:type" content="website">
  <meta property="og:site_name" content="{{ bio.name }} — {{ bio.headline }}">
  <meta property="og:locale" content="en_US">

  <!-- Twitter Card -->
  <meta name="twitter:card" content="summary_large_image">
  <meta name="twitter:title" content="{{ pageTitle or site.title }}">
  <meta name="twitter:description" content="{{ pageDescription or site.description }}">
  <meta name="twitter:image" content="{{ site.url }}{{ bio.photo | url }}">

  <!-- Canonical -->
  <link rel="canonical" href="{{ site.url }}{{ page.url }}">

  <!-- Favicon -->
  <link rel="icon" type="image/svg+xml" href="{{ '/assets/images/robot-slap-logo.svg' | url }}">
  <link rel="icon" type="image/png" sizes="32x32" href="{{ '/assets/images/favicon-32.png' | url }}">
  <link rel="apple-touch-icon" sizes="180x180" href="{{ '/assets/images/apple-touch-icon.png' | url }}">

  <!-- Preload hero image for faster LCP -->
  <link rel="preload" as="image" href="{{ '/assets/images/robot-slap-logo.svg' | url }}">

  <!-- Fonts -->
  <link rel="preconnect" href="https://fonts.googleapis.com">
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
  <link href="https://fonts.googleapis.com/css2?family=Inter:wght@400;500;600;700&family=Caveat&display=swap" rel="stylesheet">

  <link rel="stylesheet" href="{{ '/css/style.css' | url }}">

  <!-- PostHog Analytics -->
  <script>
